    "# CODE SIGNAL :- \n",
    "\n",
    "def firstNotRepeatingCharacter(s):\n",
    "    l = list(s)\n",
    "    non_repeating = [i for i,v in enumerate(l) if l.count(v) == 1]\n",
    "    if len(non_repeating) >= 1:\n",
    "        return s[non_repeating[0]]\n",
    "    else:\n",
    "        return '_'\n",
    "            "
